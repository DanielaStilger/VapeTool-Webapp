    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-cookie-consent": "^8.0.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-image-crop": "^8.6.12",
    "react-is": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
