    "@types/lodash": "^4.14.186",
    "@types/node": "18.11.7",
    "@types/qs": "^6.9.7",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "@types/react-facebook-login": "^4.1.5",
    "@types/react-helmet": "^6.1.5",
    "carlo": "^0.9.46",
    "enzyme": "^3.11.0",
    "eslint": "^8.26.0",
    "express": "^4.18.2",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
