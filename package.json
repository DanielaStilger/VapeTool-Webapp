    "@types/react-image-crop": "^8.1.3",
    "@vapetool/types": "^1.4.37",
    "antd": "^4.23.6",
    "apexcharts": "^3.36.0",
    "classnames": "^2.3.2",
    "draft-js": "^0.11.7",
    "firebase": "^8.10.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
